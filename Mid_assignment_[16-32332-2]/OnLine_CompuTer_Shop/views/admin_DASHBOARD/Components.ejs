<!DOCTYPE html>
<html>
<head>
	<title>COMPONENTS OF THIS SHOP</title>
 <link rel="stylesheet" type="text/css" href="/DEALit/css/TD_List.css">
 <script src="/DEALit/js/jquery-2.1.3.js"></script>
 <script src="/DEALit/js/pdfFromHTML.js"></script>
 <script src="/DEALit/js/jspdf.js"></script>
</head>

<body><center>
	<form>

	<table border="5">
		<tr>
			<td><div id="of"><h1><center><a href="/Components/insert">ADD NEW Components</a></center></h1></div></td>
			<td colspan="10"><h1>AVAILABLE COMPONENTS</h1>
				<div id="no"><u>TOTAL COMPONENTS: <%= td.length %></u></div>
      </td>
		</tr>

		<tr>
			<th>ID</th>
			<th>product_NAME</th>
			<th>Catagories</th>
			<th>product_Types</th>
			<th>Brands</th>
			<th>Feedbacks</th>
			<th>Cost/PRICE</th>
			<th colspan="8">ACTiONS</th>
		</tr>

		<% for(var i=0; i < td.length; i++){ %>
		<tr>
			<td><%= td[i].userID %></td>
			<td><%= td[i].c_name %></td>
			<td><%= td[i].c_catagories %></td>
			<td><%= td[i].c_types %></td>
			<td><%= td[i].c_brands %></td>
			<td><%= td[i].c_m_reviews %></td>
			<td><%= td[i].c_COST %></td>
			<td colspan="8"><center>
				<div id="link">
				<a href="/admin_DASHBOARD/Components/edit/<%= td[i].userID%>">UPDATE</a> ~
				<a href="/admin_DASHBOARD/Components/delete/<%= td[i].userID%>">DELETE</a>
			</div>
			</center></td>
		</tr>
		<% } %>

		<tr>
				<td colspan="8"><div id="of"><a href="/admin_DASHBOARD">H O M E</a></div></td>
				<td colspan="8">	<a href="javascript:pdfToHTML()">Save as PDF</a><br/></td>
		</tr>
			</table>

		</form>
</center></body>
</html>
